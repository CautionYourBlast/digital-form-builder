{
  "conditions": [
    {
      "name": "isAsap",
      "value": "licenceDetails.startKind == 'asap'"
    },
    {
      "name": "isAbsolute",
      "value": "licenceDetails.startKind == 'absolute'"
    },
    {
      "name": "isJunior",
      "value": "personalDetails.ageGroup == 'junior'"
    },
    {
      "name": "isFull",
      "value": "personalDetails.ageGroup == 'full'"
    },
    {
      "name": "isAnnual",
      "value": "licenceDetails.licenceLength == 365"
    },
    {
      "name": "isShortTerm",
      "value": "licenceDetails.licenceLength == 1 or licenceDetails.licenceLength == 8"
    },
    {
      "name": "isSalmon",
      "value": "licenceDetails.licenceType == 'salmonAndSeaTrout'"
    },
    {
      "name": "isTroutAndCourse",
      "value": "licenceDetails.licenceType == 'troutAndCourse'"
    },
    {
      "name": "hasPIPorDLA",
      "value": "concessions.hasPIPorDLA == true"
    },
    {
      "name": "hasBlueBadge",
      "value": "concessions.hasBlueBadge == true"
    },
    {
      "name": "isAsapAndFull",
      "value": "isAsap and isFull"
    },
    {
      "name": "isAnnualAndFull",
      "value": "isAnnual and isFull"
    },
    {
      "name": "isAbsoluteAndShortTerm",
      "value": "isAbsolute and isShortTerm"
    },
    {
      "name": "requiresNI",
      "value": "isAnnualAndFull and hasPIPorDLA"
    },
    {
      "name": "requiresBBCheck",
      "value": "isAnnualAndFull and hasPIPorDLA == false"
    },
    {
      "name": "requiresBB",
      "value": "requiresBBCheck and hasBlueBadge"
    }
  ],
  "startPage": "/",
  "pages": [
    {
      "path": "/start",
      "components": [
        {
          "type": "Para",
          "content": "<p class=\"govuk-body\">To get a certificate of foreign marriage recognition you need to:</p>\r\n<ul class=\"govuk-list govuk-list--bullet\">\r\n  <li>apply for a certificate online or by downloading and completing the paper application</li>\r\n  <li>make a declaration at your local consulate</li>\r\n</ul>\r\n\r\n<p class=\"govuk-body\">Both you and your partner will need to attend the appointment at the consulte to make seperate declarations.</p>",
          "options": {},
          "schema": {}
        },
        {
          "type": "Para",
          "content": "<h3 class=\"govuk-heading-m\">Before you apply</h3>\r\n\r\n<p class=\"govuk-body\"> To get a certificate of foreign marriage recogntiotion, you'll need a legalised marriage certificate.</p> \r\n\r\n<p class=\"govuk-body\">  The Spanish Foreigners’ Office (Extranjería) will usually require your marriage certificate to have been both issued and legalised in the last 3 months. </p>\r\n\r\n<p class=\"govuk-body\"> If you got married in a <a href=\"https://www.hcch.net/en/states/hcch-members\" class=\"govuk-link\">country that is part of the Hague Convention</a>, the legalisation office in that country will place a stamp or vignette on the back of the document.</p>\r\n\r\n<p class=\"govuk-body\">If not you'll need to: </p>\r\n\r\n<ul class=\"govuk-list govuk-list--bullet\">\r\n  <li>contact the authority where your marriage was registered and get a stamp or signature to validate your marriage certificate</li>\r\n  <li>take your marriage certificate to the Spanish embassy in the country you got married in so they can recognise the stamp/signature of the issuing authority</li>\r\n</ul>",
          "options": {},
          "schema": {}
        },
        {
          "type": "Para",
          "content": "<h2 class=\"govuk-heading-l\">Apply for a certificate of foreign marriage recognition certificate</h2>\r\n\r\n<p class=\"govuk-body\">Apply for a certiifcate of foreign marriage recognition online or download and complete the paper application. </p>",
          "options": {},
          "schema": {}
        }
      ],
      "next": [
        {
          "path": "/how-to-get-certifcate"
        }
      ],
      "title": "Certificate of foreign marriage recognition"
    },
