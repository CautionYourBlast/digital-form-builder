      ]
    },
    {
      "path": "/upload-page",
      "title": "Add your documents",
      "condition": "deathCertificateUpload",
      "section": "addYourDocuments",
      "components": [
        {
          "type": "Para",
          "content":
            "<h2 class=\"govuk-heading-l\">Take a picture using your phone</h2>\r\n<p class=\"govuk-body\">\r\nTake a picture of your document on a white, or neutral surface. Make sure that the picture is clear without any obstructions.</p>\r\n<p class=\"govuk-body\">\r\nAdd your picture here when you're happy with it.</p>",
          "options": {},
          "schema": {}
        },
        {
          "type": "Html",
          "content":
            "<div class=\"govuk-form-group\">\r\n  <label class=\"govuk-label\" for=\"file-upload-1\">\r\n    Upload a file\r\n  </label>\r\n  <input class=\"govuk-file-upload\" id=\"file-upload-1\" name=\"file-upload-1\" type=\"file\">\r\n</div>",
          "options": {},
          "schema": {}
        }
      ],
